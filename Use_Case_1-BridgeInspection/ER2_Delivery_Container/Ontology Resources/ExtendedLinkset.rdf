    <owl:versionInfo>v1.0</owl:versionInfo>
    <dcterms:description xml:lang="en-GB">ISO 21597 Part 2 gives a specification for more semantic relations as specializations of the generic relations of Part 1 between documentes or entries within documents within a container.</dcterms:description>
    <dcterms:title xml:lang="en-GB">The ISO 21597 ICDD Part 2 Extended Linkset ontology.</dcterms:title>
    <rdf:type rdf:resource="http://purl.org/vocommons/voaf#Vocabulary"/>
    <dcterms:creator>
      <foaf:Organization>
        <foaf:name>ISO</foaf:name>
